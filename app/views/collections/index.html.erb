<h2><%= link_to "New Collection", new_collection_path %></h2>
<hr/>
<% @all_collections.each do |collection| %>
	<h2><%= link_to collection.title, posts_collections_path(collection_id: collection.id) %> | <%= link_to "edit", edit_collection_path(collection.id) %> | <%= link_to "delete", collection_path(collection.id), method: :delete %></h2>
<% end %>